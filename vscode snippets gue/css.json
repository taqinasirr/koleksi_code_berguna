            "align-content: start/end/center/strectch/space-around/space-between/space-evenly",
            "grid-auto-flow: row/column/dense;",
            "",
            "",
            "apply to child",
            "--------------------",
            "grid-column-start/end:",
            "grid-column:",
            "",
            "grid-row-start/end:",
            "grid-row:",
            "",
            "grid-row:",
            "grid-column:",
            "",
            "justify-self: start/end/center/strectch",
            "",
            "align-self: start/end/center/strectch",
            "",
            ""
        ],
        "description": "css grid nota"
