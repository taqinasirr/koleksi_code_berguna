{
    "css core": {
        "prefix": "!!",
        "body": [
            "html{",
          "	box-sizing: border-box;",
            "}",
            "",
            "*, *::before, *::after {",
          "	box-sizing: inherit;",
            "}",
            "",
            "body{",
          "	margin: 0px;",
          "	padding: 0px;",
          "	overflow-x:hidden;",
            "}"
        ],
        "description": "csscore"
    },
    "boxSizing": {
        "prefix": "csboxsizing",
        "description": " boxSizing",
        "body": [
            "html{",
            "	box-sizing: border-box;",
            "}",
            " ",
            "*, *::before, *::after {",
            "	box-sizing: inherit;",
            "}",
