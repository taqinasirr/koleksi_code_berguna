		],
		"description": "jquery click event do effect"
	},
	"jquery click event change 1 css property": {
		"prefix": "jqclickeventchange1css",
		"description": " jquery click event change 1 css property",
		"body": [
			"$('$1').click(function(){",
			"\t$('$2').css('$3','$4');",
			"});"
		]
	},
	"jquery click event change byk css property": {
		"prefix": "jqclickeventchangebanyakcss",
		"body": [
			"$('$1').click(function(){",
